{
  "editor.fontFamily": "'PragmataPro Mono Liga', 'SF Mono', Menlo, Monaco, 'Courier New', monospace",
  "window.zoomLevel": 0,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": false,
  "editor.fontSize": 16,
  "editor.fontLigatures": true,
  "editor.minimap.maxColumn": 80,
  "editor.minimap.showSlider": "always",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.smoothScrolling": true,
  "explorer.openEditors.visible": 1,
  "breadcrumbs.enabled": true,
  "workbench.editor.showTabs": false,
  "editor.formatOnSave": true,
  "npm.enableScriptExplorer": true,
  "editor.renderIndentGuides": false,
  "editor.tabSize": 2,
  "editor.rulers": [],
  "workbench.sideBar.location": "right",
  "debug.toolBarLocation": "docked",
  "workbench.settings.editor": "json",
  "window.nativeTabs": false,
  "window.titleBarStyle": "custom",
  "workbench.settings.useSplitJSON": false,
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#ff6600",
    // "editorGroupHeader.noTabsBackground": "#282828"
  },
  "workbench.statusBar.visible": true,
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.lineNumbers": "off",
  "terminal.integrated.fontSize": 10,
  "liveshare.nameTagVisibility": "Always",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "workbench.settings.enableNaturalLanguageSearch": true,
  "workbench.enableExperiments": false,
  "extensions.showRecommendationsOnlyOnDemand": true,
  "window.restoreFullscreen": true,
  "liveshare.featureSet": "insiders",
  "workbench.tree.renderIndentGuides": "onHover",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one",
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true
  },
  "[plaintext]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 120
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "editor.renderWhitespace": "selection",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "vs-kubernetes": {
    "vs-kubernetes.draft-path": "/Users/jmc/.vs-kubernetes/tools/draft/darwin-amd64/draft",
    "vs-kubernetes.helm-path": "/Users/jmc/.vs-kubernetes/tools/helm/darwin-amd64/helm",
    "vs-kubernetes.minikube-path": "/Users/jmc/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
  },
  "todo-tree.highlights.enabled": true,
  "gitlens.currentLine.scrollable": false,
  "workbench.colorTheme": "Nord",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
}
